{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "front/client",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "front/server",
      "outputCapture": "std",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/nuxi/bin/nuxi.mjs",
      "args": [
        "dev"
      ],
    },
    {
      "request": "launch",
      "name": "data/file",
      "type": "node",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "tsx",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
    },
  ],
  "compounds": [
    {
      "name": "front",
      "configurations": [
        "front/server",
        "front/client"
      ]
    }
  ],
}
